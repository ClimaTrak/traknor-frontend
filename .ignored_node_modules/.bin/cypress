#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/cypress@13.17.0/node_modules/cypress/bin/node_modules:/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/cypress@13.17.0/node_modules/cypress/node_modules:/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/cypress@13.17.0/node_modules:/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/cypress@13.17.0/node_modules/cypress/bin/node_modules:/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/cypress@13.17.0/node_modules/cypress/node_modules:/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/cypress@13.17.0/node_modules:/mnt/c/Users/Rafael Ribeiro/Climatrak-frontend/traknor-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cypress/bin/cypress" "$@"
else
  exec node  "$basedir/../cypress/bin/cypress" "$@"
fi
